<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_item_common_problem"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:ignore="MissingConstraints">

    <TextView
        android:id="@+id/item_common_problem_title"
        android:layout_width="@dimen/px210"
        android:layout_height="@dimen/px52"
        android:maxLines="1"
        android:ellipsize="end"
        android:padding="@dimen/px5"
        android:background="@drawable/common_problem_back_selector"
        android:gravity="center"
        android:textColor="@drawable/common_problem_text_selector"
        android:textSize="@dimen/textSize15" />
</androidx.constraintlayout.widget.ConstraintLayout>